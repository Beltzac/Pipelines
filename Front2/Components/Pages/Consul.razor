@page "/consul"
@inject SignalRClientService SignalRClientService
@using Common
@using Markdig
@using Microsoft.AspNetCore.Components.Forms
@using System.Text.RegularExpressions
@using System.Reactive.Subjects
@using System.Reactive.Linq
@inject OracleDiffService OracleDiffService
@inject IToastService toastService
@rendermode InteractiveServer
@inject IJSRuntime JS
@inject BuildInfoBlazorApp.Data.BuildInfoService BuildInfoService





<h3>Build Information</h3>

<div id="myDiffElement"></div>

<button class="btn btn-primary text-nowrap" @onclick="async () => await OpenCloneInVSCode()">Open Clone Folder in VS CODE</button>



@code {

    private async ValueTask OpenCloneInVSCode()
    {
        await BuildInfoService.DownloadConsul();
    }
}
